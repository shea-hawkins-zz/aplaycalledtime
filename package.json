{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "update-schema": "babel-node --presets es2015 ./data/updateSchema.js",
    "build:webpack": "SET NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node server.js",
    "start-dev": "babel-node --presets es2015 devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.22.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "file-loader": "^0.8.4",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "@risingstack/graffiti": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "express-graphql": "^0.4.4",
    "graphql": "^0.4.13",
    "koa": "^1.1.2",
    "koa-graphql": "^0.4.4",
    "koa-mount": "^1.3.0",
    "koa-route": "^2.4.2",
    "koa-static": "^1.5.2",
    "path": "^0.12.7",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-select": "^0.9.1"
  }
}
